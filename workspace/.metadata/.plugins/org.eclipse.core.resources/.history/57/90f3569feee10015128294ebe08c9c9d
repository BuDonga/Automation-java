package com.b5m.www.Testcase;

import static org.junit.Assert.*;

import java.util.ArrayList;
import java.util.List;
import java.util.concurrent.TimeUnit;

import org.junit.After;
import org.junit.AfterClass;
import org.junit.Before;
import org.junit.BeforeClass;
import org.junit.Test;
import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;
//import org.openqa.selenium.firefox.FirefoxDriver;








import com.b5m.www.Elements.*;
import com.b5m.www.Function.*;

public class SPU_Iphone6s_Plus {
	//WebDriver driver = new FirefoxDriver();
	WebDriver driver = new ChromeDriver();
	Mainpage_ElementsFind ef = new Mainpage_ElementsFind(driver);
	Mainpage_Function mf = new Mainpage_Function(driver);
	private String url = "http://www.b5m.com/";

	@BeforeClass
	public static void setUpBeforeClass() throws Exception {
		//System.setProperty("webdriver.firefox.bin",Mainpage_Element.FirefoxBrowser);
		System.setProperty("webdriver.chrome.driver",Mainpage_Element.ChromeBrowser);
	}

	@AfterClass
	public static void tearDownAfterClass() throws Exception {
	}

	@Before
	public void setUp() throws Exception {	
		mf.driverStart(url, 3);
		//ef.AdClosed().click();
		ef.SearchLine("iphone 6s");
		ef.Submit();
		//等待小助手自动关闭
		Thread.sleep(7000);
	}

	@After
	public void tearDown() throws Exception {
		if(driver!=null){
			driver.quit();
			driver=null;
		}	
		
	}

	@Test
	public void SPU_Iphone_6s_Plus_JingDong() throws InterruptedException {
		SPU_Element_Find sef = new SPU_Element_Find(driver);
		int number =sef.ResultEqual(2, "SPU_Iphone_6s_Plus_JiongDong");
		assertEquals("iphone 6s Plus SPU结果数量不正确", 2,number);
		//等待小助手自动关闭
		Thread.sleep(7000);
		ef.SPU_Iphone6s_Plus().click();
		mf.SpuPrice("Iphone6s_Plus价格排序错误");
		ef.Iphone6s_Plus_JingDong().click();
		Thread.sleep(5000);		
		mf.PageVerify("京东商品下架_6s_Plus");
		}
	
	@Test	
	public void SPU_Iphone_6s_Plus_DangDang() throws InterruptedException {
		SPU_Element_Find sef = new SPU_Element_Find(driver);
		int number =sef.ResultEqual(2, "SPU_Iphone_6s_Plus_DangDang");
		assertEquals("iphone 6s Plus SPU结果数量不正确", 2,number);
		//等待小助手自动关闭
		Thread.sleep(7000);
		ef.SPU_Iphone6s_Plus().click();
		mf.SpuPrice("Iphone6s_Plus价格排序错误");
		ef.Iphone6s_Plus_DangDang().click();
		Thread.sleep(5000);		
		mf.PageVerify("当当商品下架_6s_Plus");
		}

	@Test	
	public void SPU_Iphone_6s_Plus_YiHaoDian() throws InterruptedException {
		SPU_Element_Find sef = new SPU_Element_Find(driver);
		int number =sef.ResultEqual(2, "SPU_Iphone_6s_Plus_YiHaoDian");
		assertEquals("iphone 6s Plus SPU结果数量不正确", 2,number);
		//等待小助手自动关闭
		Thread.sleep(7000);
		ef.SPU_Iphone6s_Plus().click();
		mf.SpuPrice("Iphone6s_Plus价格排序错误");
		ef.Iphone6s_Plus_YiHaoDian().click();
		Thread.sleep(5000);		
		assertEquals("页面跳转失败",mf.GoToNewWindow(), Mainpage_Element.YiHaoDianUrl_Plus);
		}

	@Test	
	public void SPU_Iphone_6s_Plus_TianMao() throws InterruptedException {
